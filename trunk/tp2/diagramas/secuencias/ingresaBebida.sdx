unGen:p.new(productos,cliente,hora)
unCoord:unControlPreIng.ingresar(p)
unControlPreIng:False=unControlPreIng.determinarPreparable(p)
unControlPreIng[1]:unControlListos.pedidoListo(p)
unControlListos:unControlListos.agregarPedidoListo(p)
unControlListos:p.setEstado(listo)]]></source>
